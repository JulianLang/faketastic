{
  "name": "faketastic",
  "version": "0.0.2",
  "description": "A library to help you with modelling data entities and generate them as mock data.",
  "main": "./dist/index.js",
  "private": false,
  "scripts": {
    "build": "npx rimraf ./dist && npx tsc -p .",
    "start": "ts-node ./example.ts",
    "test": "jasmine-ts --reporter=jasmine-console-reporter --config=jasmine.json",
    "test:watch": "nodemon --quiet --ext ts --exec 'jasmine-ts --reporter=jasmine-console-reporter --config=jasmine.json'"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "mock data",
    "fake data",
    "generator",
    "data modelling",
    "data specification",
    "randomized data",
    "modeller"
  ],
  "author": "jlang",
  "license": "ISC",
  "devDependencies": {
    "@types/jasmine": "^3.3.16",
    "@types/lodash": "^4.14.142",
    "@types/node": "^12.6.8",
    "jasmine": "^3.4.0",
    "jasmine-console-reporter": "^3.1.0",
    "jasmine-ts": "^0.3.0",
    "nodemon": "^1.19.3",
    "rimraf": "^3.0.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "treelike": "0.0.1"
  }
}
